<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".second">
    <com.jgabrielfreitas.core.BlurImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/Blurimage"
        android:scaleType="fitXY"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@mipmap/e"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/r1"
        android:layout_centerHorizontal="true"
        android:id="@+id/imvi"/>
    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:hint="Name"
        android:textColor="@android:color/holo_purple"
        android:id="@+id/edt_fname"
        android:paddingLeft="10dp"
        android:inputType="textPersonName"
        android:layout_below="@id/imvi"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="100dp"/>

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:hint="E-mail ID"
        android:id="@+id/edt_email"
        android:textColor="@android:color/holo_purple"
        android:paddingLeft="10dp"
        android:inputType="textEmailAddress"
        android:layout_marginLeft="100dp"
        android:layout_below="@+id/edt_fname"
        android:layout_marginTop="15dp"/>
    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:hint="Contact No."
        android:id="@+id/edt_contact"
        android:textColor="@android:color/holo_purple"
        android:paddingLeft="10dp"
        android:inputType="phone"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="100dp"
        android:layout_below="@+id/edt_email"/>

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:hint="Password"
        android:id="@+id/edt_password"
        android:inputType="numberPassword"
        android:textColor="@android:color/holo_purple"
        android:layout_marginTop="15dp"
        android:paddingLeft="10dp"
        android:layout_marginLeft="100dp"
        android:layout_below="@+id/edt_contact"/>

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:hint="Confirm Password"
        android:id="@+id/edt_cpassword"
        android:textColor="@android:color/holo_purple"
        android:inputType="numberPassword"
        android:paddingLeft="10dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="100dp"
        android:layout_below="@+id/edt_password"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Gender"
            android:textSize="20dp"
            android:id="@+id/g"
            android:textColor="@android:color/black"
            android:layout_marginTop="450dp"
            android:layout_marginLeft="20dp"/>
        <RadioGroup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/rg1">
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/male1"
                android:text="Male"
                android:layout_marginTop="450dp"
                android:layout_marginLeft="30dp"/>
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/female1"
                android:text="Female"
                android:layout_marginTop="450dp"
                android:layout_marginLeft="30dp"/>
        </RadioGroup>

    </LinearLayout>

        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="I have accepted an aggrement"
            android:id="@+id/ch"
            android:textSize="18dp"
            android:layout_marginLeft="30dp"
            android:layout_below="@+id/edt_cpassword"
            android:layout_marginTop="70dp"/>
    <Button
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Register"
        android:textSize="20dp"
        android:textColor="@android:color/white"
        android:background="@drawable/oval"
        android:id="@+id/btn_submit1"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/ch"/>


